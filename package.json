{
  "version": "0.0.1",
  "strapi": {
    "kind": "plugin",
    "name": "sso-custom",
    "displayName": "SSO Custom",
    "description": "Allows to add a full custom SSO provider to Strapi."
  },
  "name": "strapi-plugin-sso-custom",
  "description": "This plugin allows to add a full custom SSO provider to Strapi.",
  "keywords": [
    "strapi",
    "plugin",
    "sso",
    "custom",
    "provider"
  ],
  "author": {
    "name": "VO2-Brand-Experience",
    "url": "https://github.com/VO2-Brand-Experience"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/VO2-Brand-Experience/strapi-plugin-sso-custom"
  },
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "typecheck:front": "tsc --noEmit -p admin/tsconfig.json",
    "typecheck:back": "tsc --noEmit -p server/tsconfig.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.18.1",
    "@strapi/typescript-utils": "^5.18.1",
    "@stylistic/eslint-plugin": "^5.2.2",
    "@types/koa": "^2.15.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "eslint": "^9.31.0",
    "globals": "^16.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.7.1",
    "styled-components": "^6",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.7.1",
    "styled-components": "^6"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@10.13.1",
  "volta": {
    "node": "22.14.0",
    "pnpm": "10.13.1"
  }
}
